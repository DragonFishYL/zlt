.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	box-sizing:border-box;
}
page{height:auto;background:#edeef0;}
.order-head{width:100%;heigh:auto;background:#fff;position:fixed;top:0px;}
.order-model{width:33.33%;height:80rpx;line-height:80rpx;font-size:16px;font-weight:bold;float:left;text-align:center;}
.billlist{
	width:94%;
	height:300rpx;
	border-radius:5px;
	margin-top:20rpx;
	margin-left:3%;
	background:#fff;
}
.billlist-a{
	width:90%;
	height:100%;
	float:left;
}
.billlist-b{
	width:10%;
	height:100%;
	float:right;
}
.billlist-aa,.billlist-ab,.billlist-ac,.billlist-ad{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	text-indent:20rpx;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.clear{
	clear:both;
}
.billlist-b>image{
	display:block;
	width:50rpx;
	height:50rpx;
	margin-top:125rpx;
}