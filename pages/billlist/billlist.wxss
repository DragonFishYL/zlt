.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	padding:20rpx;
	box-sizing:border-box;
}
.billlist{
	width:100%;
	height:300rpx;
	border:1px solid #779eba;
	border-radius:5px;
	margin-top:20rpx;
}
.billlist-a{
	width:90%;
	height:100%;
	float:left;
}
.billlist-b{
	width:10%;
	height:100%;
	float:right;
}
.billlist-aa,.billlist-ab,.billlist-ac,.billlist-ad{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	text-indent:20rpx;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.clear{
	clear:both;
}
.billlist-b>image{
	display:block;
	width:50rpx;
	height:50rpx;
	margin-top:125rpx;
}