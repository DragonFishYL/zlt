@import "../../utils/weui.wxss";
.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	padding:20rpx;
	box-sizing:border-box;
}
.billlist{
	width:100%;
	height:300rpx;
	border-radius:5px;
}
.billlist-a{
	width:100%;
	height:100%;
}
.billlist-aa{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	margin-top:30rpx;
}
.billlist-ab{
	width:22%;
	height:75rpx;
	line-height:75rpx;
	float:left;
}
.billlist-ac{
	width:78%;
	height:75rpx;
	line-height:75rpx;
	float:right;
}
.billlist-ac input{
	height:70rpx;
	line-height:70rpx;
	border:1rpx solid #999;
	text-indent:20rpx;
}
.clear{
	clear:both;
}
button{
	margin-top:80rpx;
	height:70rpx;
	line-height:70rpx;
	color:#fff;
	font-size:14px;
}
.picker{
	border:1rpx solid #999;
	height:70rpx;
	line-height:70rpx;
	text-indent:20rpx;
}























