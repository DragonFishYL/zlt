.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	padding:20rpx;
	box-sizing:border-box;
}
.billdetail{
	width:100%;
	height:300rpx;
	border-radius:5px;
}
.billdetail-a{
	width:100%;
	height:70rpx;
	line-height:70rpx;
	font-size:16px;
	font-weight:bold;
	border-bottom:1px solid #999;
}
.billdetail-b{
	width:10%;
	height:100%;
	float:right;
}
.billlist-aa{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.billlist-bb{
	width:100%;
	height:65rpx;
	line-height:65rpx;
	color:#999;
	font-size:14px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.clear{
	clear:both;
}
.billlist-ba{
	width:100%;
	height:200rpx;
	line-height:200rpx;
	border-bottom:1px dashed #999;
}












