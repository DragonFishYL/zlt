.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	padding:20rpx;
	box-sizing:border-box;
}
.billdetail{
	width:100%;
	height:300rpx;
	border-radius:5px;
}
.billdetail-a{
	width:100%;
	height:70rpx;
	line-height:70rpx;
	font-size:16px;
	font-weight:bold;
	border-bottom:1px solid #999;
}
.billdetail-b{
	width:10%;
	height:100%;
	float:right;
}
.billlist-aa,.billlist-ab,.billlist-ac,.billlist-ad{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	text-indent:20rpx;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.clear{
	clear:both;
}
.billlist-b>image{
	display:block;
	width:50rpx;
	height:50rpx;
	margin-top:125rpx;
}