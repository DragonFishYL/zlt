.su-wrapper{
	width:100%;
	height:auto;
    position: relative;
	padding:20rpx;
	box-sizing:border-box;
}
.billlist{
	width:100%;
	height:300rpx;
	border:1px solid #779eba;
	border-radius:5px;
	margin-top:20rpx;
}
.billlist-a{
	width:100%;
	height:100%;
}
.billlist-aa{
	width:100%;
	height:75rpx;
	line-height:75rpx;
	font-size:14px;
	text-indent:20rpx;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.clear{
	clear:both;
}
.billlist-aa button{
	margin-right:10rpx;
}
.headerDetailSave{
	margin-top:20rpx;
	margin-bottom:40rpx;
	height:70rpx;
	line-height:70rpx;
	color:#fff;
	font-size:14px;
}